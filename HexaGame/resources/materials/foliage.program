vertex_program Hexa/FoliageVS hlsl
{
    source foliage.hlsl
    entry_point vert
    target vs_2_0
    
    default_params
    {
        param_named_auto viewProj viewproj_matrix
        param_named_auto time time
    }
}

fragment_program Hexa/FoliageFS hlsl
{
    source basic.hlsl
    entry_point frag
    target ps_2_0
    preprocessor_defines ALPHA_CUT
}